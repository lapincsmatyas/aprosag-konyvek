{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "aprosag-administration": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/aprosag-administration",
      "sourceRoot": "projects/aprosag-administration/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/aprosag-administration",
            "index": "projects/aprosag-administration/src/index.html",
            "main": "projects/aprosag-administration/src/main.ts",
            "polyfills": "projects/aprosag-administration/src/polyfills.ts",
            "tsConfig": "projects/aprosag-administration/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/aprosag-administration/src/favicon.ico",
              "projects/aprosag-administration/src/assets"
            ],
            "styles": [
              "projects/aprosag-administration/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/aprosag-administration/src/environments/environment.ts",
                  "with": "projects/aprosag-administration/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "aprosag-administration:build:production"
            },
            "development": {
              "browserTarget": "aprosag-administration:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "aprosag-administration:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/aprosag-administration/src/test.ts",
            "polyfills": "projects/aprosag-administration/src/polyfills.ts",
            "tsConfig": "projects/aprosag-administration/tsconfig.spec.json",
            "karmaConfig": "projects/aprosag-administration/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/aprosag-administration/src/favicon.ico",
              "projects/aprosag-administration/src/assets"
            ],
            "styles": [
              "projects/aprosag-administration/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/aprosag-administration/**/*.ts",
              "projects/aprosag-administration/**/*.html"
            ]
          }
        }
      }
    },
    "aprosag-webshop": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/aprosag-webshop",
      "sourceRoot": "projects/aprosag-webshop/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/aprosag-webshop",
            "index": "projects/aprosag-webshop/src/index.html",
            "main": "projects/aprosag-webshop/src/main.ts",
            "polyfills": "projects/aprosag-webshop/src/polyfills.ts",
            "tsConfig": "projects/aprosag-webshop/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/aprosag-webshop/src/favicon.ico",
              "projects/aprosag-webshop/src/assets"
            ],
            "styles": [
              "projects/aprosag-webshop/src/styles.scss",
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/swiper/swiper-bundle.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/aprosag-webshop/src/environments/environment.ts",
                  "with": "projects/aprosag-webshop/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "aprosag-webshop:build:production"
            },
            "development": {
              "browserTarget": "aprosag-webshop:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "aprosag-webshop:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/aprosag-webshop/src/test.ts",
            "polyfills": "projects/aprosag-webshop/src/polyfills.ts",
            "tsConfig": "projects/aprosag-webshop/tsconfig.spec.json",
            "karmaConfig": "projects/aprosag-webshop/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/aprosag-webshop/src/favicon.ico",
              "projects/aprosag-webshop/src/assets"
            ],
            "styles": [
              "projects/aprosag-webshop/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/aprosag-webshop/**/*.ts",
              "projects/aprosag-webshop/**/*.html"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "aprosag-webshop",
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  }
}
